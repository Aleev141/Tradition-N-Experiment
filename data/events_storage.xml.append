	<choice req="TNE_LIST_EVO_WEAPONS_3_NOT_FULL" blue="false" hidden="true" lvl="1">
		<text id="TNE_TEXT_EVENT_EVO_3_NOT_FULL"></text>
		<event load="OPTION_INVALID"/>
