	<choice req="TNE_POLARSTAR_3_3" blue="false" hidden="true">
		<text id="TNE_TEXT_EVENT_EVO_3_FULL"></text>
		<event load="TNE_EVO_POLARSTAR_3_TO_4"/>
	</choice>

	<choice hidden="true">
		<text id="TNE_TEXT_EVENT_EVO_RETURN"></text>
		<event load="STORAGE_CHECK"/>
	</choice>
</event>

